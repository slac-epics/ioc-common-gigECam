$(IOC):ST_SCRIPT1      30 monitor
$(IOC):ST_SCRIPT2      30 monitor
$(IOC):SUSP_TASK_CNT   30 monitor
$(IOC):SYSRESET        30 monitor

# Archive these values at the specified rate
$(IOC):DAT_MBUF_FREE   10
